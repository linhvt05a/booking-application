{
  "name": "booking_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev-android": "react-native run-android --variant=devDebug --appIdSuffix dev",
    "dev-android-release-variant": "react-native run-android --variant=devRelease --appIdSuffix dev",
    "dev-android-release": "cd android && ./gradlew clean && ./gradlew assembleDevRelease",
    "qa-android": "react-native run-android --variant=qaDebug --appIdSuffix qa",
    "qa-android-release-variant": "react-native run-android --variant=qaRelease --appIdSuffix qa",
    "qa-android-release": "cd android && ./gradlew clean && ./gradlew assembleQaRelease",
    "prod-android": "react-native run-android --variant=prodDebug --appIdSuffix prod",
    "prod-android-release-variant": "react-native run-android --variant=prodRelease --appIdSuffix prod",
    "prod-android-release": "cd android && ./gradlew clean && ./gradlew assembleProdRelease",
    "ios-dev-debug": "react-native run-ios --simulator=\"iPhone 12\" --scheme=\"booking_app_DEV\"",
    "ios-dev-release": "react-native run-ios --simulator=\"iPhone 12\" --scheme=\"booking_app_DEV\"",
    "ios-qa-debug": "react-native run-ios --simulator=\"iPhone 12\" --scheme=\"booking_app_QA\"",
    "ios-qa-release": "react-native run-ios --simulator=\"iPhone 12\" --scheme=\"booking_app_QA\"",
    "ios-prod-debug": "react-native run-ios --simulator=\"iPhone 12\" --scheme=\"booking_app_PROD\"",
    "ios-prod-release": "react-native run-ios --simulator=\"iPhone 12\" --scheme=\"booking_app_PROD\""
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.5",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^14.4.0",
    "@react-native-firebase/auth": "^14.4.0",
    "@react-native-google-signin/google-signin": "^7.0.4",
    "@react-native-twitter-signin/twitter-signin": "^1.2.0",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/react-native-vector-icons": "^6.4.10",
    "axios": "^0.25.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "formik": "^2.2.9",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-config": "^1.4.5",
    "react-native-confirmation-code-field": "^7.3.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-fbsdk-next": "^7.0.1",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-reanimated": "^2.4.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^7.2.6",
    "reactotron-react-native": "^5.0.1",
    "redux": "^4.1.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
